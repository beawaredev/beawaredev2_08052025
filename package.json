  "scripts": {
    "build": "vite build && npm run build:server",
    "build:server": "esbuild server/index.ts --bundle --platform=node --format=esm --outfile=dist/server/index.js",
    "start": "node dist/server/index.js",
    "db:push": "drizzle-kit push",
    "dev": "export TS_NODE_TRANSPILE_ONLY=true && export NODE_OPTIONS=\"--import=tsx/esm --experimental-loader=tsx/esm --loader=tsx/esm --experimental-specifier-resolution=node --no-warnings\" && npx tsx server/index.ts",
    "deploy": "npm i -g tsx && npm run start"
  },
